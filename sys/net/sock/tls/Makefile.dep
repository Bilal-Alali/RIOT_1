  USEMODULE += ztimer
  USEMODULE += ztimer_msec

  # Generic debug flag for your frontend code:
  CFLAGS += -DDEBUG_TCP_TLS

  # Generic compiler flags (optional):
  CFLAGS += -Wno-error=unused-variable -Wno-error=unused-parameter
endif

